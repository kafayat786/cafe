        "@reduxjs/toolkit": "^1.9.5",
        "ag-grid-community": "^30.0.6",
        "ag-grid-enterprise": "^30.2.1",
        "ag-grid-react": "^30.0.6",
        "axios": "^1.4.0",
        "bootstrap": "^5.3.0",
        "chart.js": "^4.3.3",
        "date-fns": "^2.30.0",
        "date-fns-tz": "^2.0.0",
        "formik": "^2.4.2",
        "jspdf": "^2.5.1",
        "jspdf-autotable": "^3.7.1",
