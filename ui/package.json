        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.1",
        "husky": "^8.0.3",
        "lint-staged": "^13.2.3",
        "prettier": "^3.0.0",
        "vite": "^4.4.0"
    },
    "lint-staged": {
        "*.{js,jsx}": [
            "eslint --cache --fix",
            "npx prettier --write"
        ]
